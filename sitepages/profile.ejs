
<style media="screen">
    a:link {
        text-decoration: none;
    }

    h6 {
        text-align: center;
    }

    .row {
        margin: 100px;
    }
</style>

<div class="container">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h6 class="panel-title">Your test History</h6>
        </div>
        <table class="table table-hover" id="dev-table">
            <thead>
                
                <th>Course</th>
                <th>Date</th>
                <th>Points Scored</th>
                <th>Passing Points</th>
                <th>Course Points</th>
                <th>Time Limit</th>
                </tr>
            </thead>
           
            <% for(i of data) {%>
                <% if(i.user == user.id){ %>
                <tr>
                    
                    <td>
                        <%= i.course.coursename %>
                    </td>
                    <td>
                        <%= i.createdAt %>
                    </td>
                    <td>
                        <%= i.marks %>
                    </td>
                    <td>
                        <%= i.course.cpassing %>
                    </td>
                    <td>
                        <%= i.course.coursepoint %>
                    </td>
                    <td>
                        <%= i.course.ctime%>s
                    </td>
                </tr><% } }%>
              
                
   
            
        </table>
        
    </div>
</div>