<style type="text/css">
    a:link {
        text-decoration: none;
    }

    .order-card {
        color: rgb(255, 255, 255);
    }

    .bg-c-blue {
        background: #04868f;
    }

    .bg-c-green {
        background: #4C51BF;
    }

    .bg-c-yellow {
        background: #F56565;
    }

    .bg-c-pink {
        background: #663a30;
    }


    .card {

        -webkit-box-shadow: 0 1px 2.94px 0.06px rgba(4, 26, 55, 0.16);
        box-shadow: 0 1px 2.94px 0.06px rgba(4, 26, 55, 0.16);
        border: 1px solid black;
        margin-bottom: 30px;
        -webkit-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;
    }

    .card .card-block {
        padding: 25px;
    }

    .order-card i {
        font-size: 26px;
    }

    .f-left {
        float: left;
    }

    .f-right {
        float: right;
    }

    header {
        left: 0px;
        right: 0px;
    }

    .uppercardtwo {
        display: flex;
        align-items: center;
        justify-content: flex-end;
        align-content: center;
        background-color: #4C51BF;
        ;
    }

    .fa-trash {
        color: red;
    }
    a{
        text-decoration: none;
        color: white;
    }
    a:hover{
        color:white;
    }
    .fa-edit {
        color: white;
    }
</style>

<br><br>

<div class="container">
    <div class="row">
    
        <% for(i of data) {%>
            <% if(i.coursename.coursename!=null){%>
            <div class="col-md-4 col-xl-6">
                <br>
                <div class="card bg-c-green order-card">
                    <a href="/gototest/?id=<%=i.id %>">
                    <div class="card-block">
                    
                        <h4 class="m-b-20">
                            <%= i.coursename.coursename %>
                        </h4>
                        <h5 class="text-left">Points <%= i.coursename.coursepoint %>
                        </h5>
                        <h5 class="text-left">Passing Points <%= i.coursename.cpassing %>
                        </h5>
                        <h5 class="text-left">Time Limit <%= i.coursename.ctime %>
                        </h5>
                        <h5 class="text-right" style="float:right">Question <%= i.coursename.coursequestion %>
                        </h5>
                    </div>
                    </a>
                </div>
            </div>

           <% } } %>  


    </div>
</div>



<script src="http://netdna.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js"></script>